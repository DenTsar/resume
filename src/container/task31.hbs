<main>
  <div class='row gy-4'>
    <section class='col-12'>
      {{#with navigation}}
        <div class='row gx-2'>
          {{#each links}}
            <div class='col-2'>
              <a href={{href}}>{{text}} </a>
            </div>
          {{/each}}
          {{#with button}}
            <div class='col-2'>
              <a
                href={{href}}
                class='btn btn-primary'
              >{{text}}</a>
            </div>
          {{/with}}

        </div>
      {{/with}}
    </section>

    <hr />

    <section class='col-12'>
      {{#with header}}
        <h1>{{title}}</h1>
        <br />
        <p>{{description}} </p>
        {{#with button}}
          <a
            href={{link}}
            class='btn btn-primary'
          >{{text}}</a>
        {{/with}}
      {{/with}}

    </section>

    <hr />

    <section class='col-12'>
      {{#with city}}
        <h2>{{title}}</h2>
        <br />
        <div class='row gx-2 gy-4'>
          {{#each cards}}
            <div class='col-5'>
              <div class='card'>
                <img
                  class='crd-img-top'
                  src={{picture.src}}
                  alt={{picture.alt}}
                />
                <h6 class='card-title'>{{title}}</h6>
                <p>{{description}}</p>
                {{#with button}}
                  <a
                    href={{href}}
                    class='btn btn-primary'
                  >{{text}}</a>
                {{/with}}

              </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
      <br />
      <hr />
    </section>

    <section class='col-12'>
      {{#with winner}}
        <h2>{{title}}</h2>
        <br />
        {{#with table}}
          <table class='table'>
            <thead>
              <tr>
                {{#each head}}

                  <th>
                    {{this}}
                  </th>
                {{/each}}
              </tr>
            </thead>
            {{#each user}}
              <tbody>
                <tr>
                  <td>{{place}} </td>
                  <td>{{name}} </td>
                  <td>{{surname}} </td>
                  <td>{{nickname}} </td>
                </tr>
              </tbody>
            {{/each}}
          </table>
        {{/with}}
      {{/with}}
    </section>

    <hr />

    <section class='col-12'>
      {{#with gallery}}
        <h2>{{title}}</h2>
        <br />
        <div class='row gx-1 gy-5'>
          {{#each cards}}

            <div class='col-4'>
              <div class='card'>
                <div class='card-header'>{{name}}</div>

                <ul class='list-group list-group-flush'>
                  {{#each items}}
                    <li
                      class='list-group-item'
                    >{{this}}</li>

                  {{/each}}
                </ul>

              </div>
            </div>

          {{/each}}
        </div>
      {{/with}}
    </section>

    <hr />

    <section>
      {{#with relaxion}}
        <div class='display-3'>{{title}}</div>
        <br />
        <div class='row gx-4 gy-4'>
          {{#each cards}}

            <div class='col-5'>
              <div
                class='card text-bg-secondary text-light'
              >
                <div class='card-header'>
                  {{header}}
                </div>
                <div class='card-title'><h5
                  >{{title}}</h5></div>
                <p>{{description}}</p>
              </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </section>

    <hr />

    <section>
      {{#with rest}}
        <div class='row gx-2 gy-4'>
          {{#each cards}}
            <div class='col-6'>
              <div class='card text-center'>
                <div class='card-header'>{{header}}</div>
                <h5 class='card-title'>{{title}}</h5>
                <p>{{description}}</p>
                <div>
                  <button class='btn btn-primary'>{{button}}
                  </button>
                </div>

                <div class='card-footer'>{{footer}}</div>

              </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </section>

    <hr />

    <section>
      {{#with proposition}}
        <div class='display-2'>{{title}}</div>
        <br />
        <div class='row gx-2 gy-4'>
          {{#each card}}
            <div class='col-5'>
              <div class='card text-bg-warning text-light'>
                <div class='card-header'> {{header}}</div>
                <h5 class='card-title'> {{title}}</h5>
                <p> {{description}}</p>

              </div>
            </div>
          {{/each}}
        </div>
      {{/with}}
    </section>

    <hr />

    {{#with footer}}
      <h5>{{title}}</h5>

      <section class='col-3'>
        {{#each navigation}}
          <a class='btn btn-link' href={{href}}>{{text}}
          </a>
        {{/each}}
      </section>
      <section class='col-3'>
        {{#each socialMedia}}
          <a
            class='btn btn-outline-dark'
            href={{link}}
          >{{name}} </a>

        {{/each}}
      </section>
      <section class='col-3'>
        {{#each partners}}
          <a
            class='btn btn-outline-info'
            href={{link}}
          >{{name}} </a>
        {{/each}}
      </section>
    {{/with}}

    {{! ------ }}

  </div>
</main>