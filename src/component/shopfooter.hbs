    <div class='row col-10 m-0 p-0 '>
      {{#each footer}}
        <div class='col-2 nav flex-column m-0 p-0 '>
          {{#each this}}
            <a href={{link}} 
              {{#if @last }} 
                class='nav-link disabled'>
              {{else}}
                class='nav-link'>
              {{/if}}  
              <h7>{{text}}</h7>
            </a>                        
          {{/each}}
        </div>
      {{/each}}

    </div>
