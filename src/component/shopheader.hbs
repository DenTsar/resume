<div class='nav' ms-0>
  {{#with navigation}}
    {{#each links}}
      <div class='nav-item'>
        {{#if href}}
          <a href={{href}} class='nav-link'>{{text}}</a>
        {{else}}
          <a href='#' class='nav-link disabled'>{{text}}</a>
        {{/if}}

      </div>
    {{/each}}
  {{/with}}
</div>

<hr class='ms-3' />